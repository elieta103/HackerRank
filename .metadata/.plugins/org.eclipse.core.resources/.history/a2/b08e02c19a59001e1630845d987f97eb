	        System.out.println(potencia(2,0));
	        System.out.println(potencia(2,1)); 
	        System.out.println(potencia(2,2));
	        System.out.println(potencia(2,4));
	    }
	    
	    private static int potencia( int base, int exponente){
	        int response = 1;
	        if(exponente!=0){
	            response =0;
	            for(int i=1; i<=exponente ; i++){
	                response = response + (base*base);
	            }
	        }
	        return response;
	    }
}
